buf1		EQU	kyflag		; buffer 1
buf2		EQU	kyflag+1	; buffer 2
buf3		EQU	kyflag+2	; buffer 3
seflag		EQU	$0E		; se flag
se_mode_flg	EQU	$0F		; effect mode
